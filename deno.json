{
  "tasks": {
    "run": "deno run --allow-read --allow-write --allow-env --allow-net --check src/main.ts",
    "dist": "deno compile --allow-read --allow-write --allow-env --allow-net --check --target x86_64-unknown-linux-gnu --output dist/asset_janny src/main.ts",
    "dist-windows": "deno compile --allow-read --allow-write --allow-env --allow-net --check --target x86_64-pc-windows-msvc --output dist/asset_janny.exe src/main.ts"
  }
}